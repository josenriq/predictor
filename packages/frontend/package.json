{
  "name": "@predictor/frontend",
  "version": "0.0.0",
  "scripts": {
    "start": "yarn ng-env:dev && ng serve",
    "build": "yarn ng-env:prod && ng build --configuration production",
    "ng-env": "node ./scripts/env.js",
    "ng-env:dev": "yarn ng-env --env=dev",
    "ng-env:prod": "yarn ng-env --env=prod",
    "build:graphql": "graphql-codegen --config graphql-codegen.yml && node ./scripts/graphql/possible-types.js && prettier --write src/app/graphql/codegen/*",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "clean": "rm -rf dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@apollo/client": "^3.6.9",
    "apollo-angular": "^4.0.1",
    "apollo-link-error": "^1.1.13",
    "apollo-link-token-refresh": "^0.4.0",
    "canvas-confetti": "^1.6.0",
    "date-fns": "^2.29.3",
    "graphql": "^16.6.0",
    "jwt-decode": "^3.1.2",
    "ng-track-by": "^1.0.1",
    "pusher-js": "^7.4.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.0",
    "@angular/cli": "~14.0.0",
    "@angular/compiler-cli": "^14.0.0",
    "@graphql-codegen/cli": "^2.12.0",
    "@graphql-codegen/typescript": "^2.7.3",
    "@types/canvas-confetti": "^1.4.3",
    "@types/jasmine": "~4.0.0",
    "autoprefixer": "^10.4.12",
    "dotenv-safe": "^8.2.0",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "postcss": "^8.4.17",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "typescript": "~4.7.2"
  }
}
